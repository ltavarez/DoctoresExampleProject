@model MantenimientoDoctor.ViewModels.EspecialidadViewModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="card">
   
    <div class="card-body">
        <h5 class="card-title">Detalle</h5>
      
    </div>
    <ul class="list-group list-group-flush">
        <li class="list-group-item"> <strong> @Html.DisplayNameFor(model => model.Nombre): </strong> @Html.DisplayFor(model => model.Nombre)</li>
       
    </ul>
    <div class="card-body">
        <a class="btn btn-outline-warning" asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
        <a class="btn btn-outline-dark" asp-action="Index">Volver atras</a>
    </div>
</div>


